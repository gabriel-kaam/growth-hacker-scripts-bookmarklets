  "devDependencies": {
    "netscape-bookmarks": "^0.1.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0"
  },
  "scripts": {
    "build": "node index.js",
    "watch": "onchange '**/*.js' -- npm run build",
    "serve": "http-server -c-1 -o bookmarklets.html",
    "start": "npm-run-all --parallel serve watch"
